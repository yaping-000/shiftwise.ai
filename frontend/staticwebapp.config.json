{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/static/*",
      "/images/*",
      "/*.{css,scss,js,png,gif,ico,jpg,svg}",
      "/api/*"
    ]
  },
  "routes": [
    {
      "route": "/api/*",
      "rewrite": "https://shiftwiseai-api.azurewebsites.net/api/:splat"
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "globalHeaders": {
    "content-security-policy": "frame-ancestors 'self';",
    "X-Frame-Options": "SAMEORIGIN",
    "X-Content-Type-Options": "nosniff"
  }
}
